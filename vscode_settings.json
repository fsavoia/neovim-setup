  "terminal.integrated.defaultProfile.linux": "zsh",
  "vim.leader": "<Space>",
  "vim.hlsearch": true,
  "vim.autoindent": true,
  // open in insert mode
  // "vim.startInInsertMode": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    // split in a new buffer vertically and horizontally
    { "before": ["leader", "s", "v"], "commands": [":vsplit"] },
    { "before": ["leader", "s", "h"], "commands": [":split"] },
    // close a buffer
    { "before": ["leader", "s", "x"], "commands": [":q"] },
    // switch b/w buffers
    { "before": ["<S-h>"], "commands": [":bprevious"] },
    { "before": ["<S-l>"], "commands": [":bnext"] },
    // navigate in panes
    {
      "before": ["<C-h>"],
      "commands": ["workbench.action.focusLeftGroup"]
    },
    {
      "before": ["<C-j>"],
      "commands": ["workbench.action.focusBelowGroup"]
    },
    {
      "before": ["<C-k>"],
      "commands": ["workbench.action.focusAboveGroup"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["workbench.action.focusRightGroup"]
    },
    // open file (fuzzy finder)
    { "before": ["leader", "f"], "commands": ["workbench.action.quickOpen"] },
    // open and close file explorer (toggle)
    {
      "before": ["leader", "e"],
      "commands": ["workbench.view.explorer"]
    },
    // open and close terminal (toggle)
    {
      "before": ["leader", "t"],
      "commands": ["workbench.action.terminal.toggleTerminal"]
    },
    // format document
    {
      "before": ["leader", "f", "d"],
      "commands": ["editor.action.formatDocument"]
    },
    // show definition preview hover with shift + k
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"]
    },
    // clean search highlights with escape (nohl)
    { "before": ["<Esc>"], "commands": [":nohl"] }
  ],
  "vim.visualModeKeyBindings": [
    // Stay in visual mode while indenting
    { "before": ["<"], "commands": ["editor.action.outdentLines"] },
    { "before": [">"], "commands": ["editor.action.indentLines"] },
    // Move selected lines while staying in visual mode
    { "before": ["J"], "commands": ["editor.action.moveLinesDownAction"] },
    { "before": ["K"], "commands": ["editor.action.moveLinesUpAction"] },
    // toggle comment selection
    { "before": ["leader", "c"], "commands": ["editor.action.commentLine"] }
  ],
  "vim.insertModeKeyBindings": [
    // press jk to exit insert mode
    { "before": ["j", "k"], "commands": ["extension.vim_escape"] }
  ],
